type Vehicle {
	id: ID!,
	modelCode: String,
	someDerivedField: String,
	brand: Brand
}

input AddVehicleInput {
	modelCode: String!,
	brandName: String
}

input UpdateVehicleInput {
	id: ID!,
	modelCode: String,
	brandId: ID!
}

type Query {
	vehicles(count: Int): [Vehicle]
	vehicle(id: ID): Vehicle
}

type Mutation {
	addVehicle(vehicle: AddVehicleInput): Vehicle
	updateVehicle(vehicle: UpdateVehicleInput): Vehicle
}
