# Add vehicle for brand name
POST http://localhost:9090/graphql
Content-Type: application/json

{
  "query": "mutation addVehicleForBrandName($brandName: String!, $vehicle: VehicleNoBrandNameInput!){  addVehicleForBrandName(brandName: $brandName, vehicle: $vehicle) {      id    modelCode    color     someDerivedField    brand {          country        name    managerName    } } }",
  "variables": {
  	"brandName": "BMW",
  	"vehicle": {
  		"modelCode": "5Series",
    	"color": "white"
  	}
  }
}

