# Update vehicle
POST http://localhost:9090/graphql
Content-Type: application/json

{
  "query": "mutation updateVehicle($id: ID!, $vehicle: VehicleInput!){  updateVehicle(id: $id, vehicle: $vehicle) {      id   modelCode   color   someDerivedField      brand {          country          name      managerName}  }}",
  "variables": {
    "id": 1,
    "vehicle": {
	    "modelCode": "A4",
	    "brandId": 1,
	    "color": "green"
    }
  }
}

