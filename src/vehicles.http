# Add vehicle
POST http://localhost:9090/graphql
Content-Type: application/json

{
  "query": "mutation addVehicle($vehicle: VehicleInput!){  addVehicle(vehicle: $vehicle) {      id    modelCode    color     someDerivedField    brand {          country        name  managerName    } } }",
  "variables": {
  	"vehicle": {
  		"brandId": "1",
  		"modelCode": "A4",
    	"color": "red"
  	}
  }
}


